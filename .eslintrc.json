{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier","plugin:prettier/recommended"],
    "plugins": ["prettier"],

    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["warn"],
        "no-unused-vars": 0,
        "no-console": 0,
        "import/extensions": 0,
        "func-names": 0,
        "no-process-exit": 0,
        "semi": 0,
        "no-undef":0,
        "no-empty":1,
        "prefer-destructuring": 0,
        "arrow-body-style": 0,
        "comma-dangle": 0,
        "max-len": ["error", { "code": 120 }],
        "no-useless-catch": 0
        

    }
}
