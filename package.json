{
  "name": "datamerge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-css": "node-sass scss/app.scss public/css/app.css",
    "build-server": "babel -d ./build ./api -s",
    "build": "npm run clean && npm run build-css && npm run build-server",
    "lint": "eslint source/ --quiet",
    "start": "node ./build/index.js",
    "debug": "node --debug ./build/index.js",
    "test": "for i in $(ls tests/); do babel-node \"./tests/${i}\" | faucet ; done",
    "validate": "npm run lint; npm run test && npm outdated --depth 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MDP-Internship/dataMerge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MDP-Internship/dataMerge/issues"
  },
  "homepage": "https://github.com/MDP-Internship/dataMerge#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "babel-cli": "^6.26.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.12"
  }
}
